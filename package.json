{
  "name": "cypress-automation-framework",
  "version": "1.0.0",
  "description": "Cypress Automation project with cucumber and API testing support.",
  "main": "index.js",
  "scripts": {
    "cypress:open:dev": "cypress open --e2e --browser chrome -e ENV=DEV",
    "cypress:open:tst:chrome": "cypress open --e2e --browser chrome -e ENV=TST",
    "cypress:open:tst:edge": "cypress open --e2e --browser edge -e ENV=TST",
    "cypress:open:tst:firefox": "cypress open --e2e --browser firefox -e ENV=TST",
    "cypress:open:prod": "cypress open --e2e --browser chrome -e ENV=PROD",
    "cypress:runner:api:tst": "cypress run --spec cypress/journeys/features/api/**/*.feature --browser chrome -e ENV=TST",
    "cypress:runner:e2e:tst": "cypress run --spec cypress/journeys/features/e2e/**/*.feature --browser chrome -e ENV=TST"
  },
  "keywords": [
    "Cypress",
    "Gherkin",
    "Cucumber",
    "API",
    "E2E",
    "Testing",
    "Automation"
  ],
  "author": "Kiuvi Services",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cucumber/cucumber": "^11.2.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@faker-js/faker": "^9.6.0",
    "cypress": "^14.5.4",
    "cypress-automation-framework": "file:",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-plugin-api": "^2.11.1",
    "esbuild": "latest",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^4.2.0",
    "multiple-cucumber-html-reporter": "^3.6.0",
    "npm-run-all": "^4.1.5"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiuviqa/base-framework-cypress.git"
  },
  "bugs": {
    "url": "https://github.com/kiuviqa/base-framework-cypress/issues"
  },
  "homepage": "https://github.com/kiuviqa/base-framework-cypress#readme"
}
